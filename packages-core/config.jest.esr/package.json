{
	"name": "@voltiso/config.jest.esr",
	"version": "1.0.0",
	"license": "UNLICENSED",
	"repository": "git://github.com/voltiso/mono.git",
	"__________": "__________",
	"sideEffects": false,
	"__________EXPORTS": "__________",
	"exports": {
		".": {
			"require": "./dist/cjs/index.js"
		},
		"./transform": {
			"require": "./dist/cjs/transform.js"
		}
	},
	"__________EXPORTS FALLBACK": "__________",
	"types": "dist/cjs/index.d.ts",
	"main": "dist/cjs/index.js",
	"__________PUBLISH WHITELIST": "__________",
	"files": [
		"dist"
	],
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________LINT": "__________",
		"lint": "run-p lint:depcheck lint:prettier lint:tsc lint:eslint",
		"lint:depcheck": "depcheck",
		"lint:prettier": "prettier --check .",
		"lint:eslint": "cross-env FULL=1 eslint --max-warnings=0 .",
		"lint:tsc": "tsc -b",
		"fix": "eslint . --fix",
		"__________BUILD": "__________",
		"build-core": "run-s clean build:build",
		"build:build": "run-p build:cjs",
		"build:cjs": "tsc -b tsconfig.build.cjs.json",
		"__________PREPARE": "__________",
		"prepare": "run-s build:cjs",
		"prepublishOnly": "run-p build-core test lint",
		"__________MISC": "__________",
		"v": "voltiso-script",
		"clean": "run-p clean:rimraf",
		"clean:rimraf": "rimraf dist"
	},
	"__________DEPS": "__________",
	"dependencies": {
		"@babel/core": "^7.18.10",
		"@babel/preset-env": "^7.18.10",
		"@testing-library/jest-dom": "^5.16.5",
		"@voltiso/config.jest.lib": "workspace:*",
		"esbuild": "^0.15.2",
		"flow-remove-types": "^2.184.0",
		"jest-environment-jsdom": "^28.1.3",
		"react": "^18.2.0",
		"react-native": "^0.69.4"
	},
	"devDependencies": {
		"@types/react": "^18.0.17",
		"@types/testing-library__jest-dom": "^5.14.5",
		"rimraf": "^3.0.2"
	}
}
