		"@voltiso/config.tsdoc": "workspace:^1.0.0",
		"@voltiso/transform": "workspace:^",
		"@voltiso/util": "workspace:^"
	},
	"__________MISC": "__________",
	"engines": {
		"node": ">=16"
	}
