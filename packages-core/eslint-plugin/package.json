{
	"name": "@voltiso/eslint-plugin",
	"version": "1.0.2",
	"license": "UNLICENSED",
	"repository": "git://github.com/voltiso/mono.git",
	"__________": "__________",
	"sideEffects": false,
	"__________EXPORTS FALLBACK": "__________",
	"types": "dist/cjs/index.d.ts",
	"main": "dist/cjs/index.js",
	"__________PUBLISH WHITELIST": "__________",
	"files": [
		"dist"
	],
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________TEST": "__________",
		"test": "echo no tests yet!",
		"__________LINT": "__________",
		"lint": "run-p lint:eslint lint:tsc",
		"lint:eslint": "cross-env FULL=1 eslint --max-warnings=0 .",
		"lint:tsc": "tsc -b",
		"depcheck": "depcheck",
		"fix": "run-s fix:prettier fix:eslint",
		"fix:prettier": "prettier . --write",
		"fix:eslint": "eslint . --fix",
		"check": "run-p fix:prettier lint test",
		"__________BUILD": "__________",
		"build": "run-s clean build:build",
		"build:build": "run-p build:cjs",
		"build:cjs": "tsc -b tsconfig.build.cjs.json",
		"__________PREPARE": "__________",
		"prepublishOnly": "run-s depcheck build+check depcheck",
		"__________MISC": "__________",
		"clean": "rimraf dist"
	},
	"__________DEPS": "__________",
	"dependencies": {
		"@typescript-eslint/utils": "^5.33.1",
		"builtins": "^5.0.1",
		"eslint-module-utils": "^2.7.4",
		"is-core-module": "^2.10.0",
		"resolve": "^1.22.1"
	},
	"devDependencies": {
		"@types/eslint": "^8.4.5",
		"@types/estree": "^1.0.0",
		"@types/is-core-module": "^2.2.0",
		"@types/resolve": "^1.20.2",
		"@voltiso/assertor.transform": "workspace:^",
		"@voltiso/config.ts.build.cjs": "workspace:^",
		"eslint": "^8.22.0",
		"import-meta-resolve": "^2.1.0",
		"rimraf": "^3.0.2",
		"typescript": "^4.7.4"
	}
}
