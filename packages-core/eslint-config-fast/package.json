{
	"name": "@voltiso/eslint-config-fast",
	"version": "1.0.0",
	"license": "UNLICENSED",
	"repository": "git://github.com/voltiso/mono.git",
	"__________": "__________",
	"sideEffects": true,
	"__________EXPORTS FALLBACK": "__________",
	"types": "dist/cjs/index.d.ts",
	"main": "dist/cjs/index.js",
	"__________PUBLISH WHITELIST": "__________",
	"files": [
		"dist"
	],
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________DEV": "__________",
		"dev": "tsc -p tsconfig.build.cjs.json --watch --noUnusedLocals false --noUnusedParameters false",
		"__________TEST": "__________",
		"test": "echo no tests yet!",
		"__________LINT": "__________",
		"lint": "run-s lint:fast lint:slow",
		"lint:fast": "run-p lint:depcheck lint:prettier",
		"lint:slow": "run-p lint:eslint lint:tsc",
		"lint:depcheck": "depcheck",
		"lint:prettier": "prettier --check .",
		"lint:eslint": "cross-env FULL=1 eslint --max-warnings=0 .",
		"lint:tsc": "tsc -b",
		"fix": "eslint . --fix",
		"__________BUILD": "__________",
		"build-core": "run-s clean build:build",
		"build:build": "run-p build:cjs",
		"build:cjs": "tsc -b tsconfig.build.cjs.json",
		"__________PREPARE": "__________",
		"prepare": "run-s build:cjs",
		"prepublishOnly": "run-p build-core lint:fast test",
		"__________MISC": "__________",
		"clean": "rimraf dist"
	},
	"__________DEPS": "__________",
	"dependencies": {
		"@typescript-eslint/parser": "^5.33.0",
		"@voltiso/config.eslint": "workspace:^",
		"@voltiso/config.eslint.lib": "workspace:^",
		"@voltiso/eslint-plugin": "workspace:^",
		"eslint-plugin-array-func": "^3.1.7",
		"eslint-plugin-chai-friendly": "^0.7.2",
		"eslint-plugin-css": "^0.6.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-destructuring": "^2.2.1",
		"eslint-plugin-editorconfig": "^4.0.2",
		"eslint-plugin-es": "^4.1.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-etc": "^2.0.2",
		"eslint-plugin-ext": "^0.1.0",
		"eslint-plugin-format-message": "^6.2.4",
		"eslint-plugin-formatjs": "^4.1.0",
		"eslint-plugin-github": "^4.3.7",
		"eslint-plugin-html": "^7.1.0",
		"eslint-plugin-i18n": "^2.1.0",
		"eslint-plugin-i18n-text": "^1.0.1",
		"eslint-plugin-i18next": "^6.0.0-4",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jest": "^26.8.2",
		"eslint-plugin-jest-async": "^1.0.3",
		"eslint-plugin-jest-dom": "^4.0.2",
		"eslint-plugin-jest-formatting": "^3.1.0",
		"eslint-plugin-jsdoc": "^39.3.6",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-json-schema-validator": "^4.0.1",
		"eslint-plugin-jsonc": "^2.3.1",
		"eslint-plugin-jsx": "^0.1.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-markdown": "^3.0.0",
		"eslint-plugin-n": "^15.2.4",
		"eslint-plugin-no-constructor-bind": "^2.0.4",
		"eslint-plugin-no-explicit-type-exports": "^0.12.1",
		"eslint-plugin-no-secrets": "^0.8.9",
		"eslint-plugin-no-unsanitized": "^4.0.1",
		"eslint-plugin-no-use-extend-native": "^0.5.0",
		"eslint-plugin-node-dependencies": "^0.9.1",
		"eslint-plugin-notice": "^0.9.10",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-putout": "^16.0.1",
		"eslint-plugin-react": "^7.30.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-native": "^4.0.0",
		"eslint-plugin-react-native-globals": "^0.1.2",
