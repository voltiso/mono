		"eslint-plugin-toml": "^0.3.1",
		"eslint-plugin-tsdoc": "^0.2.16",
		"eslint-plugin-typescript-sort-keys": "^2.1.0",
		"eslint-plugin-unicorn": "^43.0.2",
		"eslint-plugin-unused-imports": "^2.0.0",
		"eslint-plugin-yml": "^1.1.0",
		"putout": "^27.1.0"
	},
	"peerDependencies": {
		"eslint": "*",
		"prettier": "*"
	},
	"devDependencies": {
		"eslint": "^8.21.0",
		"prettier": "^2.7.1",
		"typescript": "^4.7.4"
	}
}
