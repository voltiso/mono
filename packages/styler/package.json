{
	"name": "@voltiso/styler",
	"version": "10.0.2",
	"license": "UNLICENSED",
	"repository": "git://github.com/voltiso/mono.git",
	"__________EXPORTS": "__________",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"__________EXPORTS FALLBACK": "__________",
	"types": "dist/esm/index.d.ts",
	"module": "dist/esm/index.js",
	"main": "dist/cjs/index.js",
	"__________PUBLISH WHITELIST": "__________",
	"files": [
		"dist/"
	],
	"scripts": {
		"__________TEST": "__________",
		"test": "jest",
		"__________LINT": "__________",
		"lint": "run-p lint:depcheck lint:prettier lint:tsc lint:eslint",
		"lint:depcheck": "depcheck",
		"lint:prettier": "prettier --check .",
		"lint:eslint": "eslint .",
		"lint:tsc": "run-p lint:tsc:root lint:tsc:native lint:tsc:web lint:tsc:tscOptions",
		"lint:tsc:root": "tsc -b tsconfig.json",
		"lint:tsc:native": "tsc -b test/native",
		"lint:tsc:web": "tsc -b test/web",
		"lint:tsc:tscOptions": "run-p lint:tsc:tscOptions:a lint:tsc:tscOptions:b",
		"lint:tsc:tscOptions:a": "tsc -b test/web/tsc-options/no-exactOptionalPropertyTypes",
		"lint:tsc:tscOptions:b": "tsc -b test/web/tsc-options/no-strictNullChecks",
		"__________BUILD": "__________",
		"build": "run-p build:esm build:cjs",
		"build:esm": "ttsc -b tsconfig.build.esm.json",
		"build:cjs": "ttsc -b tsconfig.build.cjs.json",
		"__________PREPARE": "__________",
		"prepublishOnly": "run-s build test",
		"__________MISC": "__________",
		"clean": "rimraf dist"
	},
	"dependencies": {
		"@voltiso/assertor": "workspace:*",
		"@voltiso/util": "workspace:*"
	},
	"peerDependencies": {
		"fela": "^12.1.2",
		"react": "^17.0.0",
		"react-fela": "^12.1.2"
	},
	"devDependencies": {
		"@jest/types": "^28.1.3",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^13.3.0",
		"@testing-library/react-native": "^11.0.0",
		"@types/react": "^18.0.12",
		"@types/react-native": "^0.69.5",
		"@types/react-test-renderer": "^18.0.0",
		"@types/testing-library__jest-dom": "^5.14.3",
		"@voltiso/config.eslint.lib": "workspace:*",
		"@voltiso/config.jest.react": "workspace:*",
		"@voltiso/config.ts": "workspace:*",
		"@voltiso/config.ts.build.cjs": "workspace:*",
		"@voltiso/config.ts.build.esm": "workspace:*",
		"@voltiso/config.ts.dom": "workspace:*",
		"csstype": "^3.1.0",
		"depcheck": "^1.4.3",
		"fela": "^12.1.2",
		"fela-native": "^12.1.2",
		"fela-plugin-typescript": "^12.1.2",
		"fela-preset-web": "^12.1.2",
		"jest": "^28.1.3",
		"jest-environment-jsdom": "^28.1.1",
		"metro-react-native-babel-preset": "^0.72.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.6.2",
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"react-fela": "^12.1.2",
		"react-native": "^0.69.3",
		"react-test-renderer": "^18.1.0",
		"rimraf": "^3.0.2",
		"typescript": "^4.7.3"
	},
	"pnpm - COPIED OVER TO MONOREPO ROOT": {
		"packageExtensions": {
			"fela-native": {
				"peerDependencies": {
					"react-native": "*"
				}
			}
		},
		"peerDependencyRules": {
			"ignoreMissing": [
				"@babel/preset-env"
			],
			"allowedVersions": {
				"react": "^18.1.0"
			}
		}
	}
}
