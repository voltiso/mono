{
	"name": "@voltiso/util.find-package-json",
	"version": "39.0.0",
	"license": "UNLICENSED",
	"repository": "git://github.com/voltiso/mono.git",
	"__________": "__________",
	"sideEffects": false,
	"__________EXPORTS FALLBACK": "__________",
	"types": "dist/esm/index.d.ts",
	"module": "dist/esm/index.js",
	"main": "dist/cjs/index.js",
	"__________PUBLISH WHITELIST": "__________",
	"files": [
		"dist"
	],
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________TEST": "__________",
		"test": "jest",
		"__________LINT": "__________",
		"lint": "run-p lint:depcheck lint:prettier lint:tsc lint:eslint",
		"lint:depcheck": "depcheck",
		"lint:prettier": "prettier --check .",
		"lint:eslint": "eslint .",
		"lint:tsc": "tsc -b",
		"fix": "eslint . --fix",
		"__________BUILD": "__________",
		"build": "run-p build:esm build:cjs",
		"build:esm": "run-s clean:esm build:esm:1",
		"build:esm:1": "ttsc -b tsconfig.build.esm.json",
		"build:cjs": "run-s clean:cjs build:cjs:1",
		"build:cjs:1": "ttsc -b tsconfig.build.cjs.json",
		"__________PREPARE": "__________",
		"prepublishOnly": "run-s build prepublishOnly:test-lint",
		"prepublishOnly:test-lint": "run-p test lint",
		"__________MISC": "__________",
		"clean": "rimraf dist",
		"clean:esm": "rimraf dist/esm",
		"clean:cjs": "rimraf dist/cjs"
	},
	"__________DEPS": "__________",
	"dependencies": {
		"@voltiso/assertor": "workspace:*",
		"@voltiso/util": "workspace:*"
	},
	"devDependencies": {
		"@voltiso/config.jest.esr": "workspace:*",
		"@voltiso/config.ts.build.cjs": "workspace:*",
		"@voltiso/config.ts.build.esm": "workspace:*",
		"depcheck": "^1.4.3",
		"rimraf": "^3.0.2"
	},
	"__________MISC": "__________",
	"engines": {
		"node": ">=16"
	}
}
