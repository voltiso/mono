{
	"name": "@voltiso/eslint-plugin",
	"version": "1.0.1",
	"license": "UNLICENSED",
	"repository": "git://github.com/voltiso/mono.git",
	"__________": "__________",
	"sideEffects": false,
	"__________EXPORTS FALLBACK": "__________",
	"types": "dist/cjs/index.d.ts",
	"main": "dist/cjs/index.js",
	"__________PUBLISH WHITELIST": "__________",
	"files": [
		"dist"
	],
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________TEST": "__________",
		"test": "echo no tests yet!",
		"__________LINT": "__________",
		"lint": "run-s lint:fast lint:slow",
		"lint:fast": "run-p lint:depcheck lint:prettier",
		"lint:slow": "run-p lint:tsc lint:eslint",
		"lint:depcheck": "depcheck",
		"lint:prettier": "prettier --check .",
		"lint:eslint": "eslint .",
		"lint:tsc": "tsc -b",
		"fix": "eslint . --fix",
		"__________BUILD": "__________",
		"build": "ttsc -b tsconfig.build.cjs.json",
		"__________PREPARE": "__________",
		"prepublishOnly": "run-s lint:fast clean build test",
		"__________MISC": "__________",
		"clean": "rimraf dist"
	},
	"__________DEPS": "__________",
	"dependencies": {
		"@typescript-eslint/utils": "^5.32.0",
		"builtins": "^5.0.1",
		"eslint-module-utils": "^2.7.3",
		"is-core-module": "^2.10.0",
		"resolve": "^1.22.1"
	},
	"devDependencies": {
		"@types/eslint": "^8.4.5",
		"@types/estree": "^1.0.0",
		"@types/is-core-module": "^2.2.0",
		"@types/resolve": "^1.20.2",
		"@voltiso/config.ts": "workspace:*",
		"@voltiso/config.ts.build.cjs": "workspace:*",
		"depcheck": "^1.4.3",
		"eslint": "^8.18.0",
		"import-meta-resolve": "^2.0.3",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"typescript": "^4.7.4"
	}
}
