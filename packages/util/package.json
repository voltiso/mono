{
  "name": "@voltiso/util",
  "version": "57.1.0",
  "license": "MIT",
  "repository": "git://github.com/voltiso/mono.git",
  "__________": "__________",
  "type": "module",
  "sideEffects": false,
  "__________EXPORTS": "__________",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/esm/index.d.ts",
        "require": "./dist/cjs/index.d.ts"
      },
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./universal-mock": {
      "types": {
        "import": "./dist/esm/universal-mock/index.d.ts",
        "require": "./dist/cjs/universal-mock/index.d.ts"
      },
      "import": "./dist/esm/universal-mock/index.js",
      "require": "./dist/cjs/universal-mock/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "universal-mock": [
        "dist/esm/universal-mock"
      ]
    }
  },
  "__________EXPORTS FALLBACK": "__________",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "__________PUBLISH WHITELIST": "__________",
  "files": [
    "dist",
    "universal-mock/"
  ],
  "__________SCRIPTS": "__________",
  "scripts": {
    "__________TURBO": "__________",
    "build": "run-p build:esm build:cjs",
    "check": "v check",
    "__________HOOKS": "__________",
    "prepublishOnly": "v prepublishOnly",
    "__________IMPL": "__________",
    "build:esm": "run-s build:esm:clean build:esm:tsc build:esm:type",
    "build:esm:clean": "rimraf dist/esm",
    "build:esm:tsc": "tspc -b tsconfig.build.esm.json",
    "build:esm:type": "echo '{\"type\":\"module\"}' > dist/esm/package.json",
    "build:cjs": "run-s build:cjs:clean build:cjs:tsc build:cjs:type",
    "build:cjs:clean": "rimraf dist/cjs",
    "build:cjs:tsc": "tspc -b tsconfig.build.cjs.json",
    "build:cjs:type": "echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json"
  },
  "__________DEPS": "__________",
  "devDependencies": {
    "@types/clone": "^2.1.4",
    "@voltiso/config.ts": "workspace:^",
    "@voltiso/config.tsdoc": "workspace:^",
    "@voltiso/transform": "workspace:^",
    "clone": "^2.1.2",
    "superjson": "^2.2.2",
    "typescript": "^5.8.3",
    "typescript-transform-paths": "^3.5.5",
    "zone.js": "^0.16.1"
  },
  "__________MISC": "__________",
  "engines": {
    "node": ">=15.0.0"
  },
  "typeCoverage": {
    "is": 98.97
  }
}
