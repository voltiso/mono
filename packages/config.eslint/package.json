    "eslint-plugin-switch-case": "^3.0.1",
    "eslint-plugin-testing-library": "^7.2.0",
    "eslint-plugin-toml": "^1.1.0",
    "eslint-plugin-tsdoc": "^0.5.0",
    "eslint-plugin-turbo": "^2.5.3",
    "eslint-plugin-unicorn": "^63.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-yml": "^3.2.0",
    "globals": "^17.3.0",
    "jsonc-eslint-parser": "^2.4.0",
    "picocolors": "^1.1.1",
    "putout": "^42.0.3",
    "yaml-eslint-parser": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@voltiso/config.ts": "workspace:^",
    "@voltiso/script": "workspace:^",
    "@voltiso/transform": "workspace:^",
    "eslint": "^9.39.2",
    "turbo": "^2.8.9",
    "typescript": "^5.8.3",
    "typescript-transform-paths": "^3.5.5"
  },
  "peerDependencies": {
    "eslint": ">= 8.18.0",
    "turbo": "*",
    "typescript": ">= 5.6.3"
  },
  "peerDependenciesMeta": {
    "turbo": {
      "optional": true
    }
  },
  "__________MISC": "__________",
  "engines": {
    "node": ">= 16",
    "pnpm": ">= 9"
  }
}
