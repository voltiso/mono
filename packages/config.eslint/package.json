    "any-eslint-parser": "^1.0.1",
    "eslint-gitignore": "^0.1.0",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-array-func": "^5.0.1",
    "eslint-plugin-chai-friendly": "^1.0.1",
    "eslint-plugin-css": "^0.12.0",
    "eslint-plugin-cypress": "^5.3.0",
    "eslint-plugin-destructuring": "^2.2.1",
    "eslint-plugin-es-x": "^9.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-ext": "^0.1.0",
    "eslint-plugin-file-extension-in-import-ts": "^2.1.0",
    "eslint-plugin-formatjs": "^6.1.3",
    "eslint-plugin-html": "^8.0.0",
    "eslint-plugin-i18n": "^2.3.0",
    "eslint-plugin-i18n-text": "^1.0.1",
    "eslint-plugin-i18next": "^6.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^29.15.0",
    "eslint-plugin-jest-async": "^1.0.3",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsdoc": "^62.5.5",
    "eslint-plugin-jsonc": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-no-use-extend-native": "^0.7.2",
    "eslint-plugin-node-dependencies": "^1.0.1",
    "eslint-plugin-notice": "^1.0.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-putout": "^31.0.0",
