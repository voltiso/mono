{
	"name": "@voltiso/util.react",
	"version": "7.8.3",
	"license": "UNLICENSED",
	"repository": "git://github.com/voltiso/mono.git",
	"__________": "__________",
	"sideEffects": false,
	"__________EXPORTS": "__________",
	"exports": {
		".": {
			"types": "./dist/esm/index.d.ts",
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"__________EXPORTS FALLBACK": "__________",
	"types": "dist/cjs/index.d.ts",
	"module": "dist/esm/index.js",
	"main": "dist/cjs/index.js",
	"__________PUBLISH WHITELIST": "__________",
	"files": [
		"dist"
	],
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________TEST": "__________",
		"test": "jest",
		"__________LINT": "__________",
		"lint": "run-p lint:depcheck lint:prettier lint:tsc lint:eslint",
		"lint:depcheck": "depcheck",
		"lint:prettier": "prettier --check .",
		"lint:eslint:full": "cross-env FULL=1 eslint --max-warnings=0 .",
		"lint:eslint:fast": "eslint --max-warnings=0 .",
		"lint:tsc": "tsc -b",
		"fix": "eslint . --fix",
		"__________BUILD": "__________",
		"build": "run-s clean build:build",
		"build:build": "run-p build:esm build:cjs",
		"build:esm": "tsc -b tsconfig.build.esm.json",
		"build:cjs": "tsc -b tsconfig.build.cjs.json",
		"__________PREPARE": "__________",
		"prepublishOnly": "run-s clean build test",
		"__________MISC": "__________",
		"clean": "rimraf dist"
	},
	"__________DEPS": "__________",
	"dependencies": {
		"@voltiso/util": "workspace:42.2.1",
		"react-intersection-observer": "^9.4.0"
	},
	"peerDependencies": {
		"react": "^18.0.0 || ^17.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.18.10",
		"@babel/plugin-proposal-decorators": "^7.18.10",
		"@babel/plugin-transform-react-jsx": "^7.18.10",
		"@babel/preset-env": "^7.18.10",
		"@testing-library/react": "^13.3.0",
		"@types/react": "^18.0.17",
		"@voltiso/config.jest.esr": "workspace:^1.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-native": "^0.69.4",
		"rimraf": "^3.0.2"
	},
	"__________MISC": "__________",
	"engines": {
		"node": ">=16"
	}
}
