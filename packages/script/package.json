{
	"name": "@voltiso/script",
	"version": "1.0.0",
	"license": "UNLICENSED",
	"repository": "git://github.com/voltiso/mono.git",
	"__________": "__________",
	"sideEffects": false,
	"__________EXECUTABLES": "__________",
	"bin": {
		"voltiso-script": "dist/cjs/_bin/v.js",
		"v": "dist/cjs/_bin/v.js"
	},
	"__________PUBLISH WHITELIST": "__________",
	"files": [
		"dist/esm",
		"dist/cjs"
	],
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________DEV": "__________",
		"dev": "tsc -p tsconfig.build.cjs.json --watch --noUnusedLocals false --noUnusedParameters false",
		"__________LINT": "__________",
		"lint": "run-p lint:depcheck lint:prettier lint:tsc lint:eslint",
		"lint:depcheck": "depcheck",
		"lint:prettier": "prettier --check .",
		"lint:eslint": "cross-env FULL=1 eslint --max-warnings=0 .",
		"lint:tsc": "tsc -b",
		"fix": "eslint . --fix",
		"__________BUILD": "__________",
		"build": "run-s clean build:build",
		"build:build": "run-p build:cjs",
		"build:cjs": "tsc -b tsconfig.build.cjs.json",
		"__________PREPARE": "__________",
		"prepublishOnly": "run-p build test lint",
		"__________MISC": "__________",
		"clean": "rimraf dist"
	},
	"__________DEPS": "__________",
	"dependencies": {
		"@voltiso/script.lib": "workspace:^",
		"@voltiso/util.esbuild": "workspace:^",
		"@voltiso/util.node": "workspace:^1.0.0"
	},
	"devDependencies": {
		"@types/node": "^18.7.6",
		"@voltiso/config.ts.build.cjs": "workspace:^",
		"@voltiso/config.ts.build.esm": "workspace:^"
	},
	"__________ENGINES": "__________",
	"engines": {
		"node": "16",
		"pnpm": "7"
	}
}
