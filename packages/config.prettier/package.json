{
	"name": "@voltiso/config.prettier",
	"version": "1.0.5",
	"license": "UNLICENSED",
	"repository": "git://github.com/voltiso/mono.git",
	"__________": "__________",
	"sideEffects": false,
	"__________EXPORTS": "__________",
	"exports": {
		".": {
			"require": "./dist/cjs/index.js"
		}
	},
	"__________EXPORTS FALLBACK": "__________",
	"types": "dist/cjs/index.d.ts",
	"main": "dist/cjs/index.js",
	"__________PUBLISH WHITELIST": "__________",
	"files": [
		"dist"
	],
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________TEST": "__________",
		"test": "jest",
		"__________LINT": "__________",
		"lint": "run-s lint:fast lint:slow",
		"lint:fast": "run-p lint:depcheck lint:prettier",
		"lint:slow": "run-p lint:tsc lint:eslint",
		"lint:depcheck": "depcheck",
		"lint:prettier": "prettier --check .",
		"lint:eslint": "eslint .",
		"lint:tsc": "tsc -b",
		"fix": "eslint . --fix",
		"__________BUILD": "__________",
		"build": "ttsc -b tsconfig.build.cjs.json",
		"__________PREPARE": "__________",
		"prepublishOnly": "run-s lint:fast clean build test",
		"__________MISC": "__________",
		"v": "voltiso-script",
		"clean": "run-p clean:rimraf",
		"clean:rimraf": "rimraf dist"
	},
	"__________DEPS": "__________",
	"peerDependencies": {
		"@voltiso/prettier-plugin-toml": "*",
		"@prettier/plugin-xml": "*",
		"prettier": "*",
		"prettier-plugin-jsdoc": "*",
		"prettier-plugin-sh": "*"
	},
	"devDependencies": {
		"@types/node": "^18.6.3",
		"@voltiso/config.jest.esr": "workspace:*",
		"@voltiso/config.ts.build.cjs": "workspace:*",
		"depcheck": "^1.4.3",
		"jest": "^28.1.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.0",
		"rimraf": "^3.0.2"
	}
}
