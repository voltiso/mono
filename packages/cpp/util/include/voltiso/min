#pragma once
#include <voltiso/_>

namespace VOLTISO_NAMESPACE {
template <class T, class... Ts>
constexpr auto min(const T &t, const Ts &...ts) noexcept {
  if constexpr (sizeof...(Ts) == 0) {
    return t;
  } else {
    auto m = min(ts...);
    return t < m ? t : m;
  }
}
} // namespace VOLTISO_NAMESPACE
