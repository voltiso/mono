{
	"name": "@voltiso/assertor",
	"version": "1.0.0",
	"license": "UNLICENSED",
	"repository": "git://github.com/voltiso/mono.git",
	"__________": "__________",
	"sideEffects": false,
	"__________EXPORTS": "__________",
	"exports": {
		".": {
			"types": "./dist/esm/index.d.ts",
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"__________EXPORTS FALLBACK": "__________",
	"types": "dist/esm/index.d.ts",
	"module": "dist/esm/index.js",
	"main": "dist/cjs/index.js",
	"__________PUBLISH WHITELIST": "__________",
	"files": [
		"dist"
	],
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________TEST": "__________",
		"test": "jest",
		"__________LINT": "__________",
		"lint": "run-p lint:depcheck lint:prettier lint:tsc lint:eslint",
		"lint:depcheck": "depcheck",
		"lint:prettier": "prettier --check .",
		"lint:eslint": "eslint .",
		"lint:tsc": "tsc -b",
		"__________BUILD": "__________",
		"build": "run-p build:esm build:cjs",
		"build:esm": "tsc -b tsconfig.build.esm.json",
		"build:cjs": "tsc -b tsconfig.build.cjs.json",
		"__________PREPARE": "__________",
		"prepublishOnly": "run-s clean prepublishOnly:rest",
		"prepublishOnly:rest": "run-p build test lint typecov",
		"__________MISC": "__________",
		"clean": "rimraf dist",
		"typecov": "run-s typecov:run typecov:fix",
		"typecov:run": "type-coverage --project tsconfig.build.esm --update",
		"typecov:fix": "prettier ./package.json --write"
	},
	"__________DEPS": "__________",
	"devDependencies": {
		"@types/assert": "^1.5.6",
		"@voltiso/config.jest.esr": "workspace:^1.0.0",
		"@voltiso/config.ts": "workspace:^1.0.0",
		"@voltiso/config.ts.build.cjs": "workspace:^1.0.0",
		"@voltiso/config.ts.build.esm": "workspace:^1.0.0",
		"depcheck": "^1.4.3",
		"jest": "^28.1.3",
		"npm-run-all": "^4.1.5",
		"type-coverage": "^2.22.0",
		"typescript": "^4.7.4",
		"zone.js": "^0.11.7"
	},
	"__________MISC": "__________",
	"engines": {
		"node": ">=16"
	},
	"dependencies": {
		"assert": "^2.0.0"
	}
}
