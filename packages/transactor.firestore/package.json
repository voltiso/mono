{
	"name": "@voltiso/transactor.firestore",
	"version": "8.0.0",
	"license": "UNLICENSED",
	"repository": "git://github.com/voltiso/mono.git",
	"__________": "__________",
	"sideEffects": false,
	"__________EXPORTS": "__________",
	"exports": {
		".": {
			"types": "./dist/esm/index.d.ts",
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"__________EXPORTS FALLBACK": "__________",
	"types": "dist/esm/index.d.ts",
	"module": "dist/esm/index.js",
	"main": "dist/cjs/index.js",
	"__________PUBLISH WHITELIST": "__________",
	"files": [
		"dist"
	],
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________TEST": "__________",
		"test": "pnpm test --filter transactor.firestore.test",
		"__________LINT": "__________",
		"lint": "run-p lint:depcheck lint:prettier lint:tsc lint:eslint",
		"lint:depcheck": "depcheck",
		"lint:prettier": "prettier --check .",
		"lint:eslint": "eslint .",
		"lint:tsc": "tsc -b tsconfig.tsc-lint.json",
		"__________BUILD": "__________",
		"build": "run-p build:esm build:cjs",
		"build:esm": "tsc -b tsconfig.build.esm.json",
		"build:cjs": "tsc -b tsconfig.build.cjs.json",
		"__________PREPARE": "__________",
		"prepublishOnly": "run-p lint prepublishOnly:rest",
		"prepublishOnly:rest": "run-s clean build test",
		"__________MISC": "__________",
		"clean": "rimraf dist"
	},
	"__________DEPS": "__________",
	"peerDependencies": {
		"@voltiso/transactor": ">= 8.0.0",
		"firebase-admin": ">= 11.0.0"
	},
	"devDependencies": {
		"@firebase/app-types": "^0.7.0",
		"@voltiso/config.ts.build.cjs": "workspace:^1.0.0",
		"@voltiso/config.ts.build.esm": "workspace:^1.0.0",
		"@voltiso/transactor": "workspace:^8.0.0",
		"depcheck": "^1.4.3",
		"firebase-admin": "^11.0.0",
		"npm-run-all": "^4.1.5"
	},
	"__________MISC": "__________",
	"engines": {
		"node": ">=16"
	}
}
