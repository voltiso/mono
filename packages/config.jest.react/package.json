{
	"name": "@voltiso/config.jest.react",
	"version": "1.0.0",
	"license": "UNLICENSED",
	"repository": "git://github.com/voltiso/mono.git",
	"__________": "__________",
	"sideEffects": false,
	"__________EXPORTS": "__________",
	"exports": {
		".": {
			"require": "./dist/cjs/index.js"
		},
		"./babel": {
			"require": "./dist/cjs/babel-config.js"
		}
	},
	"__________EXPORTS FALLBACK": "__________",
	"types": "dist/cjs/index.d.ts",
	"main": "dist/cjs/index.js",
	"__________PUBLISH WHITELIST": "__________",
	"files": [
		"dist"
	],
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________LINT": "__________",
		"lint": "run-p lint:depcheck lint:prettier lint:tsc lint:eslint",
		"lint:depcheck": "depcheck",
		"lint:prettier": "prettier --check .",
		"lint:eslint": "eslint .",
		"lint:tsc": "tsc -b",
		"fix": "eslint . --fix",
		"__________BUILD": "__________",
		"build": "ttsc -b tsconfig.build.cjs.json",
		"__________PREPARE": "__________",
		"prepublishOnly": "run-s clean prepublishOnly:rest",
		"prepublishOnly:rest": "run-p build test lint",
		"__________MISC": "__________",
		"v": "voltiso-script",
		"clean": "run-p clean:rimraf",
		"clean:rimraf": "rimraf dist"
	},
	"__________DEPS": "__________",
	"dependencies": {
		"@testing-library/jest-dom": "^5.16.4",
		"@voltiso/config.babel.lib": "workspace:*",
		"@voltiso/config.jest.lib": "workspace:*"
	},
	"peerDependencies": {
		"@babel/plugin-proposal-decorators": ">= 7.18.9",
		"@babel/plugin-transform-strict-mode": ">= 7.18.6",
		"jest": ">= 28.1.1",
		"jest-environment-jsdom": ">= 28.1.1",
		"metro-react-native-babel-preset": ">= 0.71.3"
	},
	"devDependencies": {
		"@types/babel__core": "^7.1.19",
		"@voltiso/config.ts.build.cjs": "workspace:*",
		"@voltiso/config.ts.build.esm": "workspace:*",
		"depcheck": "^1.4.3",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"typescript": "^4.7.4"
	}
}
