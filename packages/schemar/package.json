{
	"name": "@voltiso/schemar",
	"version": "12.1.2",
	"license": "UNLICENSED",
	"repository": "git://github.com/voltiso/mono.git",
	"__________": "__________",
	"sideEffects": false,
	"__________EXPORTS": "__________",
	"exports": {
		".": {
			"types": "./dist/cjs/index.d.ts",
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"__________EXPORTS FALLBACK": "__________",
	"types": "dist/cjs/index.d.ts",
	"module": "dist/esm/index.js",
	"main": "dist/cjs/index.js",
	"__________PUBLISH WHITELIST": "__________",
	"files": [
		"dist/"
	],
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________DEV": "__________",
		"dev": "tsc -p tsconfig.build.cjs.json --watch --noUnusedLocals false --noUnusedParameters false",
		"__________TEST": "__________",
		"test": "jest --silent",
		"__________LINT": "__________",
		"lint": "run-s lint:fast lint:slow",
		"lint:fast": "run-p lint:depcheck lint:prettier",
		"lint:slow": "run-p lint:eslint lint:tsc",
		"lint:depcheck": "depcheck",
		"lint:prettier": "prettier --check .",
		"lint:eslint": "cross-env FULL=1 eslint --max-warnings=0 .",
		"lint:tsc": "tsc -b",
		"__________FIX": "__________",
		"fix": "run-s fix:prettier fix:eslint",
		"fix:prettier": "prettier --write .",
		"fix:eslint": "eslint --fix .",
		"__________BUILD": "__________",
		"build": "run-s clean build:build",
		"build:build": "run-p build:esm build:cjs",
		"build:esm": "tsc -b tsconfig.build.esm.json",
		"build:cjs": "tsc -b tsconfig.build.cjs.json",
		"__________PREPARE": "__________",
		"prepublishOnly": "run-s build test",
		"__________MISC": "__________",
		"v": "voltiso-script",
		"clean": "run-p clean:rimraf",
		"clean:rimraf": "rimraf dist",
		"__________TS_TRACE": "__________",
		"trace": "run-s trace:clean trace:generate trace:analyze",
		"trace:clean": "rimraf traceDir",
		"trace:generate": "tsc --generateTrace traceDir",
		"trace:analyze": "analyze-trace traceDir"
	},
	"__________DEPS": "__________",
	"dependencies": {
		"@voltiso/util": "workspace:^"
	},
	"devDependencies": {
		"@typescript/analyze-trace": "^0.9.0",
		"@voltiso/assertor": "workspace:^",
		"@voltiso/config.jest.esr": "workspace:^",
		"@voltiso/config.ts.build.cjs": "workspace:^",
		"@voltiso/config.ts.build.esm": "workspace:^",
		"@voltiso/config.tsdoc": "workspace:^",
		"@voltiso/script": "workspace:^",
		"rimraf": "^3.0.2"
	}
}
