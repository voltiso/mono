{
	"name": "@voltiso/schemar",
	"version": "11.0.0",
	"license": "UNLICENSED",
	"repository": "git://github.com/voltiso/voltiso.git",
	"__________": "__________",
	"sideEffects": false,
	"__________EXPORTS": "__________",
	"exports": {
		".": {
			"types": "./dist/esm/index.d.ts",
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		},
		"./s": {
			"types": "./dist/esm/s/index.d.ts",
			"import": "./dist/esm/s/index.js",
			"require": "./dist/cjs/s/index.js"
		}
	},
	"typesVersions": {
		"*": {
			"s": [
				"dist/esm/s"
			]
		}
	},
	"__________EXPORTS FALLBACK": "__________",
	"types": "dist/esm/index.d.ts",
	"module": "dist/esm/index.js",
	"main": "dist/cjs/index.js",
	"__________PUBLISH WHITELIST": "__________",
	"files": [
		"dist/",
		"s/"
	],
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________TEST": "__________",
		"test": "jest --silent",
		"__________LINT": "__________",
		"lint": "run-s lint:depcheck lint:eslint lint:tsc",
		"lint:depcheck": "depcheck",
		"lint:eslint": "eslint . --max-warnings=0",
		"lint:tsc": "tsc --project tsconfig.lint.json",
		"__________BUILD": "__________",
		"build": "run-p build:esm build:cjs",
		"build:esm": "tsc -b tsconfig.build.esm.json",
		"build:cjs": "tsc -b tsconfig.build.cjs.json",
		"__________PREPARE": "__________",
		"prepublishOnly": "run-s clean prepublishOnly:rest",
		"prepublishOnly:rest": "run-p build test lint 'v compatDirs'",
		"__________MISC": "__________",
		"v": "voltiso-script",
		"clean": "run-p clean:rimraf 'v compatDirs clean'",
		"clean:rimraf": "rimraf dist",
		"//                    TODO: SCRIPT          __________TS_TRACE": "__________",
		"trace": "run-s trace:clean trace:generate trace:analyze",
		"trace:clean": "rimraf traceDir",
		"trace:generate": "tsc --generateTrace traceDir",
		"trace:analyze": "analyze-trace traceDir"
	}
}
