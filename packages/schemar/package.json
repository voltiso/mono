{
	"name": "@voltiso/schemar",
	"version": "11.0.0",
	"license": "UNLICENSED",
	"repository": "git://github.com/voltiso/mono.git",
	"__________": "__________",
	"sideEffects": false,
	"__________EXPORTS": "__________",
	"exports": {
		".": {
			"types": "./dist/esm/index.d.ts",
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"__________EXPORTS FALLBACK": "__________",
	"types": "dist/esm/index.d.ts",
	"module": "dist/esm/index.js",
	"main": "dist/cjs/index.js",
	"__________PUBLISH WHITELIST": "__________",
	"files": [
		"dist/"
	],
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________TEST": "__________",
		"test": "jest --silent",
		"__________LINT": "__________",
		"lint": "run-s lint:depcheck lint:eslint lint:tsc",
		"lint:depcheck": "depcheck",
		"lint:eslint": "cross-env NODE_OPTIONS=--max-old-space-size=8192 eslint .",
		"lint:tsc": "tsc -b",
		"__________FIX": "__________",
		"fix": "eslint . --fix",
		"__________BUILD": "__________",
		"build": "run-p build:esm build:cjs",
		"build:esm": "ttsc -b tsconfig.build.esm.json",
		"build:cjs": "ttsc -b tsconfig.build.cjs.json",
		"__________PREPARE": "__________",
		"prepublishOnly": "run-p prepublishOnly:build lint 'v compatDirs write'",
		"prepublishOnly:build": "run-s clean build test",
		"__________MISC": "__________",
		"v": "voltiso-script",
		"clean": "run-p clean:rimraf 'v compatDirs clean'",
		"clean:rimraf": "rimraf dist",
		"__________TS_TRACE": "__________",
		"trace": "run-s trace:clean trace:generate trace:analyze",
		"trace:clean": "rimraf traceDir",
		"trace:generate": "tsc --generateTrace traceDir",
		"trace:analyze": "analyze-trace traceDir"
	},
	"devDependencies": {
		"@voltiso/config.jest.esr": "workspace:^1.0.0",
		"@voltiso/config.ts": "workspace:^1.0.0",
		"@voltiso/config.ts.build.cjs": "workspace:^1.0.0",
		"@voltiso/config.ts.build.esm": "workspace:^1.0.0",
		"@voltiso/script": "workspace:^1.0.0",
		"depcheck": "^1.4.3",
		"npm-run-all": "^4.1.5"
	},
	"dependencies": {
		"@voltiso/assertor": "workspace:^1.0.0",
		"@voltiso/util": "workspace:^39.0.0"
	}
}
