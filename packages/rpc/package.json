{
	"name": "@voltiso/rpc",
	"version": "2.0.3",
	"license": "UNLICENSED",
	"repository": "git://github.com/voltiso/mono.git",
	"__________": "__________",
	"sideEffects": false,
	"__________EXPORTS": "__________",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		},
		"./client": {
			"import": "./dist/esm/client/index.js",
			"require": "./dist/cjs/client/index.js"
		},
		"./server": {
			"import": "./dist/esm/server/index.js",
			"require": "./dist/cjs/server/index.js"
		}
	},
	"typesVersions": {
		"*": {
			"client": [
				"dist/esm/client"
			],
			"server": [
				"dist/esm/server"
			]
		}
	},
	"__________EXPORTS FALLBACK": "__________",
	"types": "dist/esm/index.d.ts",
	"module": "dist/esm/index.js",
	"main": "dist/cjs/index.js",
	"__________PUBLISH WHITELIST": "__________",
	"files": [
		"dist"
	],
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________TEST": "__________",
		"test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --silent",
		"__________LINT": "__________",
		"lint": "run-s lint:depcheck lint:eslint lint:tsc",
		"lint:depcheck": "depcheck",
		"lint:eslint": "eslint .",
		"lint:tsc": "tsc -b",
		"__________BUILD": "__________",
		"build": "run-p build:esm build:cjs",
		"build:esm": "ttsc -b tsconfig.build.esm.json",
		"build:cjs": "ttsc -b tsconfig.build.cjs.json",
		"__________PREPARE": "__________",
		"prepublishOnly": "run-s build test",
		"__________MISC": "__________",
		"clean": "rimraf dist"
	},
	"dependencies": {
		"@voltiso/schemar": "workspace:*",
		"@voltiso/util": "workspace:*",
		"cross-fetch": "^3.1.5"
	},
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/node": "^18.6.3",
		"@voltiso/caller": "workspace:*",
		"@voltiso/config.jest.esr": "workspace:*",
		"@voltiso/config.ts.build.cjs": "workspace:*",
		"@voltiso/config.ts.build.esm": "workspace:*",
		"cross-env": "^7.0.3",
		"depcheck": "^1.4.3",
		"express": "^4.17.3",
		"get-port": "^6.1.2",
		"jest": "^28.1.3",
		"jest-mock-console": "^2.0.0",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"typescript": "^4.6.3"
	}
}
