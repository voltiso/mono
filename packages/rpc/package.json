{
	"name": "@voltiso/rpc",
	"version": "1.0.14",
	"license": "UNLICENSED",
	"repository": "git://github.com/voltiso/mono.git",
	"__________": "__________",
	"sideEffects": false,
	"__________EXPORTS": "__________",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		},
		"./client": {
			"import": "./dist/esm/client/index.js",
			"require": "./dist/cjs/client/index.js"
		},
		"./server": {
			"import": "./dist/esm/server/index.js",
			"require": "./dist/cjs/server/index.js"
		}
	},
	"typesVersions": {
		"*": {
			"client": [
				"dist/esm/client"
			],
			"server": [
				"dist/esm/server"
			]
		}
	},
	"__________EXPORTS FALLBACK": "__________",
	"types": "dist/esm/index.d.ts",
	"module": "dist/esm/index.js",
	"main": "dist/cjs/index.js",
	"__________PUBLISH WHITELIST": "__________",
	"files": [
		"dist"
	],
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________TEST": "__________",
		"test": "jest --silent",
		"__________LINT": "__________",
		"lint": "run-s lint:depcheck lint:eslint lint:tsc",
		"lint:depcheck": "depcheck",
		"lint:eslint": "eslint . --max-warnings=0",
		"lint:tsc": "tsc --project tsconfig.tsc-lint.json",
		"__________BUILD": "__________",
		"build": "run-p build:esm build:cjs",
		"build:esm": "tsc -b tsconfig.build.esm.json",
		"build:cjs": "tsc -b tsconfig.build.cjs.json",
		"__________PREPARE": "__________",
		"prepublishOnly": "run-s clean prepublishOnly:rest",
		"prepublishOnly:rest": "run-p build test lint",
		"__________MISC": "__________",
		"clean": "rimraf dist"
	},
	"dependencies": {
		"@voltiso/schemar": "workspace:^11.0.0",
		"@voltiso/util": "workspace:^39.0.0",
		"cross-fetch": "^3.1.5"
	},
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/node": "^17.0.23",
		"@voltiso/caller": "workspace:^1.0.0",
		"@voltiso/config.jest.esr": "workspace:^1.0.0",
		"@voltiso/config.ts.build.cjs": "workspace:^1.0.0",
		"@voltiso/config.ts.build.esm": "workspace:^1.0.0",
		"depcheck": "^1.4.3",
		"express": "^4.17.3",
		"jest": "^27.5.1",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"typescript": "^4.6.3"
	}
}
