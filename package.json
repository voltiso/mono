{
  "private": true,
  "__________SCRIPTS": "__________",
  "scripts": {
    "__________TURBO - PER-PACKAGE": "__________",
    "dev": "FORCE_COLOR=1 turbo run dev",
    "build": "FORCE_COLOR=1 turbo run build:cjs build:esm",
    "build:cjs": "FORCE_COLOR=1 turbo run build:cjs",
    "build:esm": "FORCE_COLOR=1 turbo run build:esm",
    "test": "FORCE_COLOR=1 turbo run test",
    "lint": "FORCE_COLOR=1 turbo run lint:tsc lint:eslint",
    "depcheck": "FORCE_COLOR=1 turbo run depcheck",
    "check": "v checkWorkspace",
    "__________PREPARE WORKSPACE": "__________",
    "prepare": "FORCE_COLOR=1 run-s prepare:ts-patch prepare:script prepare:run",
    "prepare:ts-patch": "FORCE_COLOR=1 ts-patch install -s",
    "prepare:script": "FORCE_COLOR=1 turbo run build:cjs --filter=@voltiso/script",
    "prepare:run": "v prepareWorkspace"
  },
  "__________DEPS": "__________",
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/node": "^18.11.0",
    "@typescript/analyze-trace": "^0.9.1",
    "@voltiso/config.eslint.lib": "workspace:^",
    "@voltiso/config.jest.esr": "workspace:^",
    "@voltiso/config.jest.lib": "workspace:^",
    "@voltiso/config.prettier": "workspace:^",
    "@voltiso/config.ts": "workspace:^",
    "@voltiso/eslint-config": "workspace:^",
    "@voltiso/eslint-config-fast": "workspace:^",
    "@voltiso/script": "workspace:^",
    "@voltiso/transform": "workspace:^",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.3",
    "eslint": "*",
    "gnomon": "^1.5.0",
    "jest": "^29.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.0.2",
    "turbo": "^1.5.6",
    "type-coverage": "^2.22.0",
    "typescript": "^4.8.4",
    "typescript-transform-paths": "^3.3.1",
    "zone.js": "^0.11.8"
  },
  "__________ENGINES": "__________",
  "engines": {
    "node": "16",
    "pnpm": "7"
  },
  "packageManager": "pnpm@7.6.0",
  "pnpm": {
    "overrides": {
      "jest-worker": "29.0.3",
      "@eslint/eslintrc": "1.3.2"
    },
    "packageExtensions": {
      "fela-native": {
        "peerDependencies": {
          "react-native": "*"
        }
      },
      "@radix-ui/react-checkbox": {
        "peerDependencies": {
          "@types/react": "*"
        }
      },
      "@radix-ui/react-primitive": {
        "peerDependencies": {
          "@types/react": "*"
        }
      }
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "*"
      }
    }
  }
}
