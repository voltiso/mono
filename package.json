    "@voltiso/config.jest": "workspace:^",
    "@voltiso/config.jest.lib": "workspace:^",
    "@voltiso/config.prettier": "workspace:^",
    "@voltiso/config.ts": "workspace:^",
    "@voltiso/eslint-config": "workspace:^",
    "@voltiso/eslint-config-fast": "workspace:^",
    "@voltiso/script": "workspace:^",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.5",
    "eslint": "^8.48.0",
    "gnomon": "^1.5.0",
    "jest": "^29.6.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
     "turbo": "1.10.3 || 1.10.4-1.10.13-broken-for-test-command",
    "type-coverage": "^2.26.1",
    "typescript": "^5.2.2"
  },
