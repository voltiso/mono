{
	"private": true,
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________TURBO - PER-PACKAGE": "__________",
		"dev": "turbo run dev",
		"build": "turbo run build-core build",
		"build:core": "turbo run build-core",
		"build:rest": "turbo run build",
		"test": "turbo run test",
		"lint": "turbo run lint --concurrency 2 # high RAM usage!",
		"check": "run-s test lint",
		"depcheck": "run-p depcheck:w depcheck:packages",
		"depcheck:w": "depcheck",
		"depcheck:packages": "turbo run lint:depcheck",
		"fix": "turbo run fix",
		"clean": "turbo run clean",
		"__________WORKSPACE LEVEL": "__________",
		"lint:w": "cross-env NODE_OPTIONS=--max_old_space_size=16384 eslint .",
		"fix:w": "run-s fix:w:prettier fix:w:eslint",
		"fix:w:prettier": "prettier --write .",
		"fix:w:eslint": "cross-env NODE_OPTIONS=--max_old_space_size=16384 eslint --fix ."
	},
	"__________DEPS": "__________",
	"devDependencies": {
		"@types/jest": "^28.1.6",
		"@types/node": "^18.7.1",
		"@voltiso/assertor.transform": "workspace:^1.0.0",
		"@voltiso/config.eslint.lib": "workspace:^1.0.0",
		"@voltiso/config.jest.esr": "workspace:^1.0.0",
		"@voltiso/config.jest.lib": "workspace:^1.0.0",
		"@voltiso/config.prettier": "workspace:^1.0.6",
		"@voltiso/config.ts": "workspace:^1.0.0",
		"@voltiso/config.ts.build.cjs": "workspace:^1.0.0",
		"@voltiso/config.ts.build.esm": "workspace:^1.0.0",
		"@voltiso/eslint-config": "workspace:^1.1.1",
		"cross-env": "^7.0.3",
		"depcheck": "^1.4.3",
		"eslint": "*",
		"jest": "^28.1.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"ts-node": "^10.9.1",
		"ttypescript": "^1.5.13",
		"turbo": "^1.4.3",
		"type-coverage": "^2.22.0",
		"typescript": "^4.7.4",
		"typescript-transform-paths": "^3.3.1",
		"zone.js": "^0.11.7"
	},
	"__________ENGINES": "__________",
	"engines": {
		"node": "^16.0.0",
		"pnpm": "^7.0.0"
	},
	"packageManager": "pnpm@7.6.0",
	"pnpm": {
		"packageExtensions": {
			"fela-native": {
				"peerDependencies": {
					"react-native": "*"
				}
			}
		},
		"peerDependencyRules": {
			"allowedVersions": {
				"react": "*"
			}
		}
	}
}
