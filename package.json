		"lint:tsc:eopt:f": "tsc -b src/modules/compiler-options/exactOptionalPropertyTypes/test-false",
		"__________BUILD": "__________",
		"build": "run-s clean build:tsup build:gen-compat-dirs",
		"build:tsup": "tsup",
		"build:gen-compat-dirs": "esr script/gen-compat-dirs.ts",
