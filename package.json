{
  "private": true,
  "__________SCRIPTS": "__________",
  "scripts": {
    "__________TURBO - PER-PACKAGE": "__________",
    "dev": "cross-env FORCE_COLOR=1 turbo run dev",
    "build": "cross-env FORCE_COLOR=1 turbo run build:cjs build:esm",
    "build:cjs": "cross-env FORCE_COLOR=1 turbo run build:cjs",
    "build:esm": "cross-env FORCE_COLOR=1 turbo run build:esm",
    "test": "cross-env FORCE_COLOR=1 turbo run test",
    "lint": "cross-env FORCE_COLOR=1 turbo run lint:tsc lint:eslint",
    "depcheck": "cross-env FORCE_COLOR=1 turbo run depcheck",
    "check": "v checkWorkspace",
    "__________PREPARE WORKSPACE": "__________",
    "prepare": "cross-env FORCE_COLOR=1 run-s prepare:ts-patch prepare:script prepare:run",
    "prepare:ts-patch": "cross-env FORCE_COLOR=1 ts-patch install -s",
    "prepare:script": "cross-env FORCE_COLOR=1 turbo run build:cjs --filter=@voltiso/script",
    "prepare:run": "v prepareWorkspace"
  },
  "__________DEPS": "__________",
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.18",
    "@typescript/analyze-trace": "^0.9.1",
    "@voltiso/config.eslint.lib": "workspace:^",
    "@voltiso/config.jest": "workspace:^",
    "@voltiso/config.jest.lib": "workspace:^",
    "@voltiso/config.prettier": "workspace:^",
    "@voltiso/config.ts": "workspace:^",
    "@voltiso/eslint-config": "workspace:^",
    "@voltiso/eslint-config-fast": "workspace:^",
    "@voltiso/script": "workspace:^",
    "@voltiso/transform": "workspace:^",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.3",
    "eslint": "*",
    "gnomon": "^1.5.0",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.0.2",
    "turbo": "^1.6.3",
    "type-coverage": "^2.24.1",
    "typescript": "^4.9.3",
    "typescript-transform-paths": "^3.4.4"
  },
  "__________ENGINES": "__________",
  "engines": {
    "node": "16",
    "pnpm": "7"
  },
  "packageManager": "pnpm@7.22.0",
  "pnpm": {
    "overrides": {
      "typescript": "4.8.4 || 4.9.4-is-broken",
      "react": "18.1.0 || react-native-not-updated-to-peer-newer",
      "react-dom": "18.1.0 || react-native-not-updated-to-peer-newer",
      "react-test-renderer": "18.1.0 || react-native-not-updated-to-peer-newer",
      "@eslint/eslintrc": "1.3.2"
    },
    "packageExtensions": {
      "fela-native": {
        "peerDependencies": {
          "react-native": "*"
        }
      },
      "@radix-ui/react-checkbox": {
        "peerDependencies": {
          "@types/react": "*"
        }
      },
      "@radix-ui/react-primitive": {
        "peerDependencies": {
          "@types/react": "*"
        }
      }
    }
  }
}
