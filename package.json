{
	"name": "@voltiso/ts-util",
	"version": "8.0.0",
	"license": "UNLICENSED",
	"repository": "git://github.com/voltiso/ts-util.git",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./array": {
			"types": "./dist/array/index.d.ts",
			"import": "./dist/array/index.mjs",
			"require": "./dist/array/index.js"
		},
		"./boolean": {
			"types": "./dist/boolean/index.d.ts",
			"import": "./dist/boolean/index.mjs",
			"require": "./dist/boolean/index.js"
		},
		"./object": {
			"types": "./dist/object/index.d.ts",
			"import": "./dist/object/index.mjs",
			"require": "./dist/object/index.js"
		},
		"./string": {
			"types": "./dist/string/index.d.ts",
			"import": "./dist/string/index.mjs",
			"require": "./dist/string/index.js"
		}
	},
	"typesVersions": {
		"*": {
			"array": [
				"dist/array"
			],
			"boolean": [
				"dist/boolean"
			],
			"object": [
				"dist/object"
			],
			"string": [
				"dist/string"
			]
		}
	},
	"types": "dist/index.d.ts",
	"module": "dist/index.mjs",
	"main": "dist/index.js",
	"files": [
		"dist/",
		"array/",
		"boolean/",
		"object/",
		"string/"
	],
	"scripts": {
		"__________TEST": "__________",
		"test": "jest",
		"__________LINT": "__________",
		"lint": "run-s lint:eslint lint:tsc",
		"lint:eslint": "eslint . --max-warnings=0",
		"lint:tsc": "tsc --project tsconfig.lint.json",
		"__________BUILD": "__________",
		"prebuild": "run-s clean",
		"build": "run-s build:tsup build:gen-compat-dirs",
		"build:tsup": "tsup",
		"build:gen-compat-dirs": "esr script/gen-compat-dirs.ts",
		"__________PREPARE": "__________",
		"prepare": "run-s build",
		"prepublishOnly": "run-s test lint",
		"__________MISC": "__________",
		"clean": "rimraf dist database"
	},
	"devDependencies": {
		"@types/jest": "^27.4.1",
		"@types/node": "^17.0.23",
		"@typescript-eslint/eslint-plugin": "^5.17.0",
		"@typescript-eslint/parser": "^5.17.0",
		"esbuild-runner": "^2.2.1",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jest": "^26.1.3",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "^27.5.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.6.1",
		"rimraf": "^3.0.2",
		"tsup": "^5.12.1",
		"typescript": "^4.6.3"
	}
}
