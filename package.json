{
	"private": true,
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________TURBO - PER-PACKAGE": "__________",
		"dev": "turbo run dev",
		"build": "turbo run build-core build",
		"build:core": "turbo run build-core",
		"build:rest": "turbo run build",
		"test": "turbo run test",
		"lint": "turbo run lint # high RAM usage!",
		"check": "run-s test lint",
		"depcheck": "run-p depcheck:w depcheck:packages",
		"depcheck:w": "depcheck",
		"depcheck:packages": "turbo run lint:depcheck",
		"fix": "turbo run fix",
		"clean": "turbo run clean",
		"__________WORKSPACE LEVEL": "__________",
		"lint:w": "cross-env NODE_OPTIONS=--max_old_space_size=16384 eslint .",
		"fix:w": "run-s fix:w:prettier fix:w:eslint",
		"fix:w:prettier": "prettier --write .",
		"fix:w:eslint": "cross-env NODE_OPTIONS=--max_old_space_size=16384 eslint --fix .",
		"__________PREPARE": "__________",
		"prepare": "ts-patch install -s"
	},
	"__________DEPS": "__________",
	"devDependencies": {
		"@types/jest": "^28.1.6",
		"@types/node": "^18.7.2",
		"@voltiso/assertor.transform": "workspace:*",
		"@voltiso/config.eslint.lib": "workspace:*",
		"@voltiso/config.jest.esr": "workspace:*",
		"@voltiso/config.jest.lib": "workspace:*",
		"@voltiso/config.prettier": "workspace:*",
		"@voltiso/config.ts": "workspace:*",
		"@voltiso/config.ts.build.cjs": "workspace:*",
		"@voltiso/config.ts.build.esm": "workspace:*",
		"@voltiso/eslint-config": "workspace:*",
		"@voltiso/eslint-config-fast": "workspace:*",
		"cross-env": "^7.0.3",
		"depcheck": "^1.4.3",
		"eslint": "*",
		"jest": "^28.1.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"ts-node": "^10.9.1",
		"ts-patch": "^2.0.2",
		"turbo": "^1.4.3",
		"type-coverage": "^2.22.0",
		"typescript": "^4.7.4",
		"typescript-transform-paths": "^3.3.1",
		"zone.js": "^0.11.8"
	},
	"__________ENGINES": "__________",
	"engines": {
		"node": ">= 16",
		"pnpm": ">= 7"
	},
	"packageManager": "pnpm@7.6.0",
	"pnpm": {
		"packageExtensions": {
			"fela-native": {
				"peerDependencies": {
					"react-native": "*"
				}
			}
		},
		"peerDependencyRules": {
			"allowedVersions": {
				"react": "*"
			}
		}
	}
}
