{
	"private": true,
	"__________SCRIPTS": "__________",
	"scripts": {
		"__________TURBO - PER-PACKAGE": "__________",
		"dev": "turbo run dev",
		"build": "turbo run build",
		"test": "turbo run test",
		"lint": "turbo run lint",
		"__________WORKSPACE + PACKAGES": "__________",
		"w:clean": "v cleanWorkspace",
		"__________PREPARE WORKSPACE": "__________",
		"prepare": "run-s prepare:ts-patch prepare:script prepare:run",
		"prepare:ts-patch": "ts-patch install -s",
		"prepare:script": "turbo run build:cjs --filter @voltiso/script",
		"prepare:run": "v prepareWorkspace"
	},
	"__________DEPS": "__________",
	"devDependencies": {
		"@types/jest": "^28.1.7",
		"@types/node": "^18.7.6",
		"@voltiso/assertor.transform": "workspace:^",
		"@voltiso/config.eslint.lib": "workspace:^",
		"@voltiso/config.jest.esr": "workspace:^",
		"@voltiso/config.jest.lib": "workspace:^",
		"@voltiso/config.prettier": "workspace:^",
		"@voltiso/config.ts": "workspace:^",
		"@voltiso/eslint-config": "workspace:^",
		"@voltiso/eslint-config-fast": "workspace:^",
		"@voltiso/script": "workspace:^",
		"@voltiso/transform": "workspace:^",
		"@voltiso/util.node": "workspace:^",
		"cross-env": "^7.0.3",
		"depcheck": "^1.4.3",
		"eslint": "*",
		"jest": "^28.1.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"ts-node": "^10.9.1",
		"ts-patch": "^2.0.2",
		"turbo": "^1.4.3",
		"type-coverage": "^2.22.0",
		"typescript": "^4.7.4",
		"typescript-transform-paths": "^3.3.1",
		"zone.js": "^0.11.8"
	},
	"__________ENGINES": "__________",
	"engines": {
		"node": "16",
		"pnpm": "7"
	},
	"packageManager": "pnpm@7.6.0",
	"pnpm": {
		"packageExtensions": {
			"fela-native": {
				"peerDependencies": {
					"react-native": "*"
				}
			}
		},
		"peerDependencyRules": {
			"allowedVersions": {
				"react": "*"
			}
		}
	}
}
